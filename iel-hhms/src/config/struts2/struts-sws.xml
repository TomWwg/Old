<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<!--登陆模块	-->
	<package name="login" extends="sys" namespace="/web">
		<action name="*" class="com.sws.action.LoginAction"
			method="{1}">
			<result name="login" type="json">
				<param name="excludeNullProperties">true</param>
			</result>
			<result name="logout">
				login.jsp
			</result>
			<result name="index">
				index.jsp
			</result>
			<result name="indexHome">
				indexHome.jsp
			</result>
			<result name="goHomeInfo">
				homeInfo.jsp
			</result>
		</action>
	</package>
    <!--用户管理	-->
	<package name="user" extends="sys" namespace="/web/module/user">
		<action name="*" class="com.sws.action.UserAction"
			method="{1}">
			<result name="pageUser">
				userList.jsp
			</result>
			<result name="personalCenter">
				personalCenter.jsp
			</result>
			<result name="changePasswordUI">
				changePasswordUI.jsp
			</result>
			<result name="showDialog"> showForm.jsp </result>
			<result name="addUserShowDialog">addUserShowForm.jsp</result>
			<result name="addFavoriteDialog"> addFavorite.jsp </result>
		</action>
	</package>
	<!--角色管理	-->
	<package name="role" extends="sys" namespace="/web/module/role">
		<action name="*" method="{1}" class="com.sws.action.RoleInfoAction">
    		<result name="page"> roleList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    		<result name="assignMenuDialog"> assignMenu.jsp </result>
    	</action>
	</package>
	<!--首页帮助	-->
	<package name="help" extends="sys" namespace="/web/module/help">
		<action name="*" method="{1}" class="com.sws.action.HelpAction">
    		<result name="page"> help.jsp </result>
    		<result name="contactPage">contact.jsp</result>
    	</action>
	</package>
	<!--职工管理	-->
	<package name="staff" extends="sys" namespace="/web/module/staff">
		<action name="*" method="{1}" class="com.sws.action.StaffInfoAction">
    		<result name="page"> staffList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    	</action>
	</package>
	<!--设备管理	-->
	<package name="device" extends="sys" namespace="/web/module/device">
		<action name="*" method="{1}" class="com.sws.action.DeviceInfoAction">
		    <result name="selectGroupTree">selectGroupTree.jsp </result>
		    <result name="updateSelectGroupTree">updateSelectGroupTree.jsp </result>
		    <result name="addBedTree">addBedTree.jsp </result>
    		<result name="page"> deviceList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    		<result name="queryAlarmList"> queryAlarmList.jsp </result>
    		<result name="queryApList"> apList.jsp </result>
    		<result name="rfidDialog">addRfidForm.jsp </result>
    		<!-- yxnne add :把原来在device管理里面的组织树拉出来-->
    		<result name="department">departmentInfo.jsp </result>
    	</action>
	</package>	
	<!--日志查询	-->
	<package name="log" extends="sys" namespace="/web/module/log">
		<action name="*" class="com.sws.action.LogInfoAction"
			method="{1}">
			<result name="logInfoList">
				logInfoList.jsp
			</result>
		</action>
	</package>
	<!--手卫生统计查询	-->
	<package name="WashHand" extends="sys" namespace="/web/module/washHand">
		<action name="*" class="com.sws.action.WashHandLogAction"
			method="{1}">
			<result name="todayEventList">
				todayEventList.jsp
			</result>
			<result name="eventList">
				eventList.jsp
			</result>
			<result name="rateByStaffList">
				rateByStaff.jsp
			</result>
			<result name="rateByStaffTypeList">
				rateByStaffType.jsp
			</result>
			<result name="rateByDepartList">
				rateByDepart.jsp
			</result>
			<result name="eventCompareList">
				eventCompare.jsp
			</result>
			<!-- yxnne- 新功能：针对走廊中的：外门液瓶识别器 -->
			<result name="eventOutDoorStatistic">
				eventOutDoorStatistic.jsp
			</result>
			<!-- yxnne end -->
			<!-- yxnne- 新功能：次数统计 -->
			<result name="washTimesStatistic">
				washTimesStatistic.jsp
			</result>
			<result name="washTimesStatistic_pro">
				washTimesStatistic_pro.jsp
			</result>
			<!-- yxnne end -->
			<result name="getStaff" type="json">
				<param name="contentType">text/html</param> 
				<param name="root">json</param>  
			</result>
			<result name="getStaffByType" type="json">
				<param name="contentType">text/html</param> 
				<param name="root">json</param>  
			</result>
		</action>
	</package>
	<!--日志查询(查询统计)	-->
	<package name="sigLog" extends="sys" namespace="/web/module/signalLog">
		<action name="*" class="com.sws.action.SignalLogAction"
			method="{1}">
			<result name="page">
				signalLog.jsp
			</result>
		</action>
	</package>
	<!--报表管理	-->
	<package name="reportManage" extends="sys" namespace="/web/module/reportManage">
		<action name="*" method="{1}" class="com.sws.action.ReportManagementAction">
    		<result name="goWashChart"> washChart.jsp </result>
    		<result name="goRateByTime"> timeList.jsp </result>
    		<result name="goShowScreen"> showScreen.jsp </result>
    	</action>
	</package>
	<!--公告管理	-->
	<package name="notice" extends="sys" namespace="/web/module/notice">
		<action name="*" method="{1}" class="com.sws.action.NoticeInfoAction">
    		<result name="page"> noticeList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    		<result name="showNotice"> showNotice.jsp </result>
    	</action>
	</package>
	<!--人员定位管理	-->
	<package name="Location" extends="sys" namespace="/web/module/location">
		<action name="*" method="{1}" class="com.sws.action.LocationAction">
    		<result name="goStaffLocation"> staffLocation.jsp </result>
    		<result name="allStaffLocation"> allStaffLocation.jsp </result>
    	</action>
	</package>
	<!--串口读取	-->
	<package name="serial" extends="sys" namespace="/web/module/serial">
		<action name="*" method="{1}" class="com.sws.action.SerialAction">
    		<result name="serialSet"> serialSet.jsp </result>
    	</action>
	</package>
	<!--Ajax	-->
	<package name="ajax" extends="sys" namespace="/web/module">
		<action name="*" method="{1}" class="com.sws.action.AjaxAction">
    		<result name="serialSet"> serialSet.jsp </result>
    	</action>
	</package>
	<!--洗手液	-->
	<package name="sanitizer" extends="sys" namespace="/web/module/sanitizer">
		<action name="*" method="{1}" class="com.sws.action.SanitizerCountAction">
    		<result name="sanitizerByStaffList"> sanitizerByStaffList.jsp </result>
    		<result name="sanitizerByDeptList">sanitizerByDept.jsp</result>
    	</action>
	</package>
	<!--导出	-->
	<package name="exportFun" extends="sys" namespace="/web/module/exportFun">
		<action name="*" method="{1}" class="com.sws.action.ExportFunctionAction">
    	</action>
	</package>
	<!--APP接口	-->
	<package name="app" extends="json-default" namespace="/web/app">
		<action name="*" class="com.sws.action.AppAction"
			method="{1}">
			<result type="json"> 
			<param name="root">dataMap</param>     
            </result> 
		</action>
	</package>
	<!--app新闻管理	-->
	<package name="appNews" extends="sys" namespace="/web/module/appNews">
		<action name="*" method="{1}" class="com.sws.action.AppNewsAction">
    		<result name="page"> appNewsList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    	</action>
	</package>
	<!--AP设置	-->
	<package name="apSet" extends="sys" namespace="/web/module/apSet">
		<action name="*" class="com.sws.action.ApSetAction"
			method="{1}">
			<result name="page">
				apList.jsp
			</result>
			<result name="apSet">
				apSet.jsp
			</result>
		</action>
	</package>
	<!--参数设置	-->
	<package name="parameter" extends="sys" namespace="/web/module/parameter">
		<action name="*" method="{1}" class="com.sws.action.ParameterInfoAction">
    		<result name="page"> parameterList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    	</action>
	</package>
	<!--排班管理	-->
	<package name="schedule" extends="sys" namespace="/web/module/schedule">
		<action name="*" method="{1}" class="com.sws.action.ScheduleAction">
    		<result name="page"> scheduleList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    	</action>
	</package>
	<!--排班日期管理	-->
	<package name="dateTemplate" extends="sys" namespace="/web/module/dateTemplate">
		<action name="*" method="{1}" class="com.sws.action.DateTemplateAction">
    		<result name="page">dateTemplateList.jsp </result>
    		<result name="showDialog"> showForm.jsp </result>
    	</action>
	</package>
	
	<!--license管理	-->
	<package name="license" extends="sys" namespace="/web/module/license">
		<action name="*" class="com.sws.action.LicenseAction" method="{1}">
			<interceptor-ref name="fileUpload"> </interceptor-ref>
            <interceptor-ref name="defaultStack"/>
		    <param name="savePath">/WEB-INF/classes</param>
		    <result name="licenseUpload">/licenseUpload.jsp</result>
		</action>
		
	</package>
	
</struts>