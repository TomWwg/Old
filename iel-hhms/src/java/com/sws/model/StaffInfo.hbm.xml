<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="staff_info" name="com.sws.model.StaffInfo">    <comment>职工信息表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_staff_info</param>      </generator>    </id>    <property name="name" type="string">      <column name="name" length="60" not-null="true">        <comment>职工姓名</comment>      </column>    </property>    <property name="no" type="string">      <column name="no" length="64" not-null="true">        <comment>职工工号</comment>      </column>    </property>    <property name="category" type="string">      <column name="category" length="20" not-null="true">        <comment>类别</comment>      </column>    </property>    <many-to-one cascade="none" class="com.sws.model.GroupTree" lazy="false" not-null="true" name="groupTree">      <column name="group_id">        <comment>所属组织</comment>      </column>    </many-to-one>    <many-to-one cascade="none" class="com.sws.model.GroupTree" lazy="false" not-null="true" name="locationTree">      <column name="location_id">        <comment>位置树id</comment>      </column>    </many-to-one>    <property name="rfid" type="string">      <column name="rfid" length="64">        <comment>胸牌编号</comment>      </column>    </property>    <property name="deviceNo" type="string">      <column name="device_no" length="20" >        <comment>识别器编号</comment>      </column>    </property>    <property name="deviceType" type="string">      <column name="device_type" length="20">        <comment>识别器类型</comment>      </column>    </property>    <property name="sex" type="string">      <column name="sex" length="10">        <comment>性别</comment>      </column>    </property>    <property name="tel" type="string">      <column name="tel" length="20">        <comment>电话</comment>      </column>    </property>    <property name="duty" type="string">      <column name="duty" length="20">        <comment>职务</comment>      </column>    </property>    <property name="jobTitle" type="string">      <column name="job_title" length="20">        <comment>职称</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>