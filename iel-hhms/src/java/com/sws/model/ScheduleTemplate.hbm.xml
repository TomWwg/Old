<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="schedule_template" name="com.sws.model.ScheduleTemplate">    <comment>排班模板表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_schedule_template</param>      </generator>    </id>    <property name="name" type="string">      <column name="name" length="60" not-null="true">        <comment>排班模板名称</comment>      </column>    </property>    <property name="startHm" type="java.util.Date">      <column name="start_hm">        <comment>开始时分</comment>      </column>    </property>    <property name="endHm" type="java.util.Date">      <column name="end_hm">        <comment>结束时分</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>