<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="ap_set" name="com.sws.model.ApSet">    <comment>AP设置表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_ap_set</param>      </generator>    </id>    <property name="no" type="string">      <column name="no" length="60" not-null="true">        <comment>AP编号</comment>      </column>    </property>    <property name="ip" type="string">      <column name="ip" length="60" not-null="true">        <comment>IP地址</comment>      </column>    </property>     <property name="apStatus" type="integer">      <column name="ap_status">        <comment>0正常 1断开</comment>      </column>    </property>    <property name="postCode" type="string">      <column name="post_code" length="60">        <comment>邮编</comment>      </column>    </property>    <property name="hospitalNo" type="string">      <column name="hospital_no" length="60">        <comment>医院编码</comment>      </column>    </property>    <property name="departNo" type="string">      <column name="depart_no" length="60">        <comment>科室编号</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>