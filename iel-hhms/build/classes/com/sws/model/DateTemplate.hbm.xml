<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="date_template" name="com.sws.model.DateTemplate">    <comment>排班日期表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_date_template</param>      </generator>    </id>    <property name="name" type="string">      <column name="name" length="60" not-null="true">        <comment>排班日期名称</comment>      </column>    </property> 	<many-to-one cascade="none" class="com.sws.model.ScheduleTemplate" lazy="false" not-null="true" name="scheduleTemplate">      <column name="schedule_id">        <comment>所属排班模板</comment>      </column>    </many-to-one>    <property name="staffIds" type="string">      <column name="staff_ids" length="100" not-null="true">        <comment>排班模板的应用人</comment>      </column>    </property>    <property name="day" type="string">      <column name="day" length="100" not-null="true">        <comment>排班的应用日期</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>