<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="user_info" name="com.sws.model.UserInfo">    <comment>用户信息表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_user_info</param>      </generator>    </id>    <property name="name" type="string">      <column name="name" length="60" not-null="true">        <comment>用户名</comment>      </column>    </property>    <property name="password" type="string">      <column name="password" length="64" not-null="true">        <comment>密码</comment>      </column>    </property>    <property name="tel" type="string">      <column name="tel" length="20">        <comment>电话号码</comment>      </column>    </property>    <property name="expireTime" type="java.util.Date">      <column name="expire_time">        <comment>过期时间</comment>      </column>    </property>    <property name="remark" type="string">      <column name="remark" length="1024">        <comment>备注</comment>      </column>    </property>    <property name="rfId" type="string">      <column name="rf_id">        <comment>胸牌ID</comment>      </column>    </property>    <many-to-one cascade="none" class="com.sws.model.GroupTree" lazy="false" not-null="true" name="groupTree">      <column name="group_id">        <comment>所属组织</comment>      </column>    </many-to-one>    <many-to-one cascade="none" class="com.sws.model.RoleInfo" lazy="false" not-null="true" name="roleInfo">      <column name="role_id">        <comment>所属角色</comment>      </column>    </many-to-one>    <property name="userStatus" type="integer">      <column name="user_status">        <comment>0正常 1禁用2过期</comment>      </column>    </property>     <property name="lastLoginTime" type="java.util.Date">      <column name="last_login_time">        <comment>最后登陆时间</comment>      </column>    </property>    <property name="loginCounts" type="integer">      <column name="login_counts">        <comment>登陆次数</comment>      </column>    </property>    <property name="favoriteIds" type="string">      <column name="favorite_Ids" length="1024">        <comment>收藏信息</comment>      </column>    </property>    <property name="departIds" type="string" not-null="true">      <column name="depart_Ids" length="1024">        <comment>所属部门</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>