<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="parameter_info" name="com.sws.model.ParameterInfo">    <comment>参数信息表</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_parameter_info</param>      </generator>    </id>    <property name="type" type="integer">      <column name="type" not-null="true">        <comment>参数类型</comment>      </column>    </property>    <property name="key" type="string">      <column name="key" length="64" not-null="true">        <comment>参数唯一的key</comment>      </column>    </property>    <property name="value" type="string">      <column name="value" length="64" not-null="true">        <comment>参数设置值（显示值）</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>