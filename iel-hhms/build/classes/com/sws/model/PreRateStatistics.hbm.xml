<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>  <class table="pre_rate_statistics" name="com.sws.model.PreRateStatistics">    <comment>预统计表(月日)</comment>    <id unsaved-value="null" name="id">      <column name="id">        <comment>主键</comment>      </column>      <generator class="native" >      	<param name="sequence">seq_pre_rate_statistics</param>      </generator>    </id>    <property name="recordType" type="integer">      <column name="record_type" not-null="true">        <comment>记录类：0日 1月</comment>      </column>    </property>    <property name="timeStr" type="string">      <column name="time_str" length="20" not-null="true">        <comment>记录时间</comment>      </column>    </property>    <property name="rfid" type="string">      <column name="rfid" length="20" not-null="true">        <comment>胸牌编号</comment>      </column>    </property>    <property name="effectWash" type="integer">      <column name="effect_wash" not-null="true">        <comment>有效手卫生次数</comment>      </column>    </property>    <property name="inRoom" type="integer">      <column name="in_room" not-null="true">        <comment>进入病区未手卫生次数</comment>      </column>    </property>    <property name="outRoom" type="integer">      <column name="out_room" not-null="true">        <comment>离开病区未手卫生次数</comment>      </column>    </property>    <property name="beforeInBed" type="integer">      <column name="before_in_bed" not-null="true">        <comment>接近患者前未手卫生次数</comment>      </column>    </property>    <property name="longOutBed" type="integer">      <column name="long_out_bed" not-null="true">        <comment>长时离开患者未手卫生次数</comment>      </column>    </property>    <property name="rate" type="string">      <column name="rate" length="10" not-null="true">        <comment>依从率</comment>      </column>    </property>    <property name="status" update="false" type="long">      <column name="status">        <comment>状态</comment>      </column>    </property>    <property name="creator" update="false" type="string">      <column name="creator" length="64">        <comment>创建者</comment>      </column>    </property>    <property name="createTime" update="false" type="java.util.Date">      <column name="create_time">        <comment>创建时间</comment>      </column>    </property>    <property name="updateTime" type="java.util.Date">      <column name="update_time">        <comment>更新时间</comment>      </column>    </property>  </class></hibernate-mapping>