<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ taglib prefix="s" uri="/struts-tags"%><!doctype html><html><head><%@ include file="/common/commons.jsp"%><style type="text/css">/*标题样式*/.btn-padding{	margin-top:5px;	text-align:left;}.titleLabel{	background:#14b6f6;	color:white;	font-size:18px;	padding-left:10px;	padding-right:10px;	padding-top:2px;	padding-bottom:5px;	margin-top:5px;	margin-left:20px;}/*信息区域*/.filterForm{	height:130px;}.itemSection{	margin-top:10px;}.itemLable{	margin-left:120px;	margin-top:2px;	margin-bottom:2px;	font-size:15px;}/*按钮*/btn_sections{	 	 width:400px;	 height:400px;	 background:#f6b614;}.one_btn_section{	width:120px;	height:150px;	background:#eeeeee;	float:left;	margin-right:10px;	border-radius:5px;	margin-left:120px;	margin-top:40px;}.one_btn_img_section{	width:60px;	height:60px;	margin:0 auto ;}.one_btn_section img{	margin:0 auto ;	margin-top:30px;}.one_btn_section .text{	margin-top:40px;}/*hover*/.one_btn_section:hover{	border-style: solid; 	border-width: 1px 1px 1px 1px;	border-color: #aaaaaa;	box-shadow: 5px 5px 3px #aaaaaa;	background:white;	cursor:pointer;}</style></head><body>     <!-- 菜单栏  -->	<div class="breadLine displayNone">		<ul class="breadcrumbs auto-navigation" menu-code="${menuCode}"></ul>	</div>	<!-- 主页面 -->	<div class="wrapper grid-s6m0">		<div class="col-main">			<div class="main-wrap"				style="overflow-y: inherit;">				<!-- 工具栏  -->				<div class="btn-toolbar" style="height: 36px; position: relative;">					<div class=btn-padding>						<label class="titleLabel">选中科室信息</label>					</div>				</div>				<!-- 查询条件  -->				<div class="fluid filterForm">					<!-- 					<form id="filterForm" action="" method="post"						class="form-horizontal" history="true">						<input type="hidden" id="treeId" name="queryEntity.treeId" value="1"/>						<ul class="formRow" style="padding: 0px">							<li class="control-group auto-by-filter"><label								class="control-label">编号：</label>								<div class="controls">									<input type="text" name="queryEntity.str1">								</div></li>							<li class="control-group auto-by-filter" style="display:none"><label								class="control-label">名称：</label>								<div class="controls">									<input type="text" name="queryEntity.str2">								</div></li>							 <li class="control-group auto-by-filter"><label								class="control-label">类型：</label>								<div class="controls">								  <select id="type" name="queryEntity.str3">							        <option value="-1">---全部---</option>							        <c:forEach items="${deviceTypeMap}" var="tList">                                      <option value="${tList.key}">${tList.value}</option>                                    </c:forEach>								      </select>								</div></li> 						</ul>						<div class=btn-set>							<a class="buttonM bBlue" type="button" id="data_filtering">查询</a>						</div>					</form>					-->					<div class="itemSection">					<div class="itemLable">名&nbsp;&nbsp;&nbsp;称&nbsp;:&nbsp;&nbsp;<label id="info_name">感控科x</label></div>					<div class="itemLable">上一级&nbsp;:&nbsp;&nbsp;<label id="info_parent">医院</label></div>					<div class="itemLable">科室级别&nbsp;:&nbsp;&nbsp;<label id="info_level">全部科室级别</label></div>					<div class="itemLable">备&nbsp;&nbsp;&nbsp;注&nbsp;:&nbsp;&nbsp;<label id="info_remark"></label></div>					</div>									</div>				<!-- 数据列表                  <div class="datatable">			         <table id="table"> </table>			         <div id="dataTablePage"></div>		          </div>		           -->		          <div class="datatable" >			         <div class=btn-padding>						<label class="titleLabel">操作</label>					</div>					<!-- 树上方按钮 									<div class="btn-toolbar clearfixBORDER-BOTTOM: #ccc 1px dashed;display:none;">					<a class="buttonS"  id="addTree-btn" style="margin-top: 5px;display:none;"						href="javascript:void(0);"><span class="ico i-add"></span><span>添加</span></a>					<a class="buttonS"  id="editTree-btn" style="margin-top: 5px;display:none;"						href="javascript:void(0);"><span class="ico i-edit"></span><span>修改</span></a>					<a class="buttonS"  id="delTree-btn" style="margin-top: 5px;display:none;"						href="javascript:void(0);"><span class="ico i-del"></span><span>删除</span></a>  					</div>					-->					<!-- 操作区间 -->					<div class="btn_sections">						<div class="one_btn_section" id="addTree-btn">							<div class="one_btn_img_section">								<img src="../../../images/add_one_tree.png"  alt="" 							  	width="45px" height="45px" />							</div>							<div class="text">								<label>增加子节点</label>							</div>														 						</div>												<div class="one_btn_section" id="editTree-btn">							<div class="one_btn_img_section">								<img src="../../../images/modify_one_tree.png"  alt="" 							  	width="45px" height="45px"/>							</div>							<div class="text">								<label>修改当前节点</label>							</div>							  						</div>												<div class="one_btn_section" id="delTree-btn">							<div class="one_btn_img_section">								<img src="../../../images/delete_one_tree.png"  alt="" 							  	width="50px" height="50px"/>							</div>							<div class="text">								<label>删除当前节点</label>							</div>							   						</div>					</div>							          </div>		           			</div>		</div>		<!-- 左侧树 -->		<div class="col-sub">			<div class="treeview">				<form id="treeForm" action="">					<input type="hidden" id="topGroupId" name="topGroupId"						value="${topGroupId}">					<!-- 存放groupId -->				</form>				<!-- 树上方按钮 -->								<ul id="zTreea" class="ztree" style="margin: 0px 10px"></ul>			</div>		</div>	</div>	<script type="text/javascript" src="js/deviceList.js"></script></body></html>